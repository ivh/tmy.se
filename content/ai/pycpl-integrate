    50→                               CPPFLAGS=&quot;$CPPFLAGS -D_POSIX_C_SOURCE=200809L -D_XOPEN_SOURCE=500&quot;
    51→                               CFLAGS=&quot;$CFLAGS -std=c99&quot;
    52→                            ])
    53→ESO_PROG_CC_FLAG([fno-common], [CFLAGS=&quot;$CFLAGS -fno-common&quot;])
    54→
    55→ESO_ENABLE_DEBUG(no)
    56→ESO_ENABLE_STRICT(no)
