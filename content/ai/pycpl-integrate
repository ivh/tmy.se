   136→        # wcslib doesn&#x27;t support out-of-tree builds well, build in-source
   137→        env = os.environ.copy()
   138→        # Set proper LDFLAGS and CFLAGS instead of CFITSIOLIB/CFITSIOINC
   139→        env[&quot;LDFLAGS&quot;] = f&quot;-L{install_dir / &#x27;lib&#x27;}&quot;
